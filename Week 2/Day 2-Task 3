//break
for (let i = 2; i <= 20; i++) {
    let isPrime = true;
for (let j = 2; j < i; j++) {
    if (i % j === 0) {
          isPrime = false;
          break
}
}
if (isPrime) {
        console.log("First prime number found:", i);
        break; // Exit the loop after finding the first prime
}
}
//continue 
for (let i = 1; i <= 20; i++) {
    if (i % 2 === 0) {
        continue; // Skip even numbers
    }
    console.log("Odd number:", i);
}
